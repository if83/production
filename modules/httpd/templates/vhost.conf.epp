<%- | $port,
      $server_name,
      $server_alias,
      $document_root,
      $error_log,
      $reguests_log,
| -%>

<VirtualHost *: <%= $port %>
ServerName <%= $server_name %>
<% if $server_alias { %> ServerAlias <%= $server_alias %><% } %>
DocumentRoot <%= $document_root %>
<% if $error_log { %> ErrorLog <%= $error_log %><% } %>
<% if $reguests_log { %> CustomLog <%= $reguests_log, %><% } %>
</VirtualHost>
